# OneTakeAnalytics — copy to .env and fill in values
# cp .env.example .env

# Reco service (gRPC port)
GRPC_PORT=50052

# Analytics Ingest (use a different port when running both services from one .env — set in docker/run)
# GRPC_PORT=50051

# ClickHouse (docker-compose maps native to host 9001; use 9001 when connecting from host)
CLICKHOUSE_HOST=localhost
CLICKHOUSE_PORT=9001
CLICKHOUSE_USER=default
CLICKHOUSE_PASSWORD=default

# Reco: Core API URL (for similar_by_tags)
CORE_API_URL=http://localhost:5000

# Analytics Ingest (batch)
BATCH_SIZE=200
BATCH_INTERVAL_SEC=1.5
